{
  "name": "recenter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "detox:build": "detox build",
    "detox:test": "detox test",
    "nativewind:build": "tailwindcss -i ./global.css -o ./nativewind.css --config ./tailwind.config.js",
    "nativewind:watch": "tailwindcss -i ./global.css -o ./nativewind.css --config ./tailwind.config.js --watch",
    "brand:generate": "node scripts/generate-brand-assets.js",
    "brand:build": "npm run brand:generate && npm run nativewind:build",
    "supabase:push": "npx supabase db push",
    "supabase:gen-types": "npx supabase gen types typescript --schema public --project-id %SUPABASE_PROJECT_ID% > src/models/supabase.ts"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "^2.1.0",
    "@supabase/supabase-js": "^2.58.0",
    "expo": "~54.0.10",
    "expo-auth-session": "^7.0.8",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.9",
    "expo-file-system": "^19.0.15",
    "expo-font": "~14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "^17.0.7",
    "expo-notifications": "^0.32.11",
    "expo-router": "~6.0.8",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.7",
    "i18next": "^25.5.2",
    "immer": "^10.1.3",
    "ky": "^1.11.0",
    "nativewind": "^4.2.1",
    "posthog-react-native": "^4.7.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.17",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/detox": "^17.14.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "detox": "^20.42.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "sharp": "^0.34.4",
    "ts-jest": "^29.4.4",
    "typescript": "~5.9.2"
  },
  "private": true
}
